@model IEnumerable<E_Commerce.Models.Product_Order>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Order.orderName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product.productName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Order.orderName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Product.productName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.productOrder_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.productOrder_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.productOrder_id })
        </td>
    </tr>
}

</table>
